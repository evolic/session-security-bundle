{
  "name": "loculus/session-security-bundle",
  "type": "library",
  "license": "MIT",
  "description": "Bundle improving session security in Symfony based applications",
  "authors": [
    {
      "name": "Tomasz Kuter",
      "email": "tkuter@loculus.pl"
    }
  ],
  "require": {
    "php": "^8.0",
    "psr/log": "^3.0",
    "symfony/config": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/event-dispatcher": "^6.0",
    "symfony/http-foundation": "^6.0",
    "symfony/http-kernel": "^6.0",
    "symfony/security-core": "^6.0"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "^3.12",
    "phpmd/phpmd": "^2.12",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5",
    "slevomat/coding-standard": "^8.2",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "Loculus\\SessionSecurityBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Loculus\\SessionSecurityBundle\\Tests\\": "tests/"
    }
  }
}
